{
  "name": "attachmentgenie-ssh",
  "version": "3.0.0",
  "author": "Bram Vogelaar <bram@attachmentgenie.com>",
  "summary": "Puppet ssh Module",
  "license": "MIT",
  "source": "https://github.com/attachmentgenie/attachmentgenie-ssh",
  "project_page": "https://github.com/attachmentgenie/attachmentgenie-ssh",
  "issues_url": "https://github.com/attachmentgenie/attachmentgenie-ssh/issues",
  "description": "Module for configuring ssh.",
  "tags": [
    "ssh",
    "ssh-client",
    "ssh-server"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14",
        "16"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.13.0 < 5.0.0"
    }
  ]
}
